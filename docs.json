{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "RankZero Docs",
  "colors": {
    "primary": "#0f766e",
    "light": "#5eead4",
    "dark": "#0f766e"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "languages": [
      {
        "language": "en",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              "onboarding"
            ]
          },
          {
            "group": "Dashboard",
            "pages": [
              "d/dashboard",
              "d/prompts",
              "d/competitors",
              "d/sources",
              "d/chats"
            ]
          },
          {
            "group": "Metrics",
            "pages": [
              "metrics/visibility",
              "metrics/mentions",
              "metrics/sentiment",
              "metrics/usage",
              "metrics/citations"
            ]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "Home",
        "href": "https://www.rankzero.io"
      },
      {
        "label": "Log in",
        "href": "https://www.rankzero.io/dashboard"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Start Ranking with AI",
      "href": "https://www.rankzero.io/onboarding"
    }
  },
  "logo": {
    "light": "/logo/rankzero-logo-light.svg",
    "dark": "/logo/rankzero-logo-dark.svg"
  },
  "footer": {
  },
   "redirects": [
    {
      "source": "/",
      "destination": "https://rankzero.io/docs",
      "permanent": true
    },
    {
      "source": "/d/:path*",
      "destination": "https://rankzero.io/docs/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "destination": "https://rankzero.io/docs/:path*",
      "permanent": true
    }
  ]
}